<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.cashdonatefragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/ofwhite">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="279dp"
            android:layout_height="0dp"
            android:layout_marginTop="112dp"
            android:layout_marginBottom="395dp"
            android:text="رقم الهاتف : 0548368113/ 0545972009"
            android:textColor="@color/teal_700"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/bakbtn"
            app:layout_constraintStart_toStartOf="@+id/textView4"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="205dp"
            android:layout_height="125dp"
            android:layout_marginBottom="292dp"
            android:text="مكان الحمعيه : الناصره الحي الشرقي كقابل ازياء السندس"
            android:textColor="@color/teal_700"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="162dp"
            android:layout_height="88dp"
            android:layout_marginEnd="124dp"
            android:layout_marginBottom="152dp"
            android:text="نتشرف بكم "
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textColor="@color/teal_700"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <Button
            android:id="@+id/bakbtn"
            android:layout_width="120dp"
            android:layout_height="45dp"
            android:layout_marginStart="57dp"
            android:layout_marginBottom="57dp"
            android:backgroundTint="@color/babypink"
            android:text="back"
            android:textColor="@color/black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>